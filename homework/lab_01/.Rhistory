knitr::opts_chunk$set(echo = TRUE)
# Turn off scientific notation
options(scipen=999)
# Load the tidyverse. If you have not installed the tidyverse already, remove the # from the next line and run it first.
# install.packages('tidyverse')
library(tidyverse)
umd_courses <- read_rds("lab_01.rds")
umd_courses |>
group_by(term, department) |>
summarise(
count_classes = n()
) |>
arrange(term, department)
umd_courses |>
group_by(term) |>
summarise(
count_classes = n(),
total_seats = sum(seats),
mean_seats = mean(seats),
median_seats = median(seats)
) |>
arrange(desc(mean_seats))
umd_courses |>
group_by(term, department) |>
summarise(
count_classes = n(),
total_seats = sum(seats),
mean_seats = mean(seats),
median_seats = median(seats)
) |>
arrange(desc(mean_seats))
umd_courses |>
group_by(term) |>
summarise(
count_classes = n()
) |>
arrange(desc(count_classes))
umd_courses |>
group_by(id) |>
summarise(
count_classes = n()
) |>
arrange(desc(count_classes))
umd_courses %>%
drop_na(instructors) %>%
group_by(instructors, department) %>%
summarise(
count_classes = n()
) %>%
arrange(desc(count_classes))
umd_courses %>%
group_by(department, classes, term)
umd_courses %>%
group_by(department, seats, term)
umd_courses %>%
group_by(department, seats, term) %>%
summarise(
count_classes = n()
)
umd_courses %>%
group_by(department, seats, term) %>%
filter(term==201801) %>%
summarise(
count_classes = n()
)
